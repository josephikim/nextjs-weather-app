{
  "name": "nextjs-weather-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run lint --fix && npm run check-types && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "seed-db": "npx prisma db seed",
    "vercel-build": "prisma generate && prisma db push --force-reset && prisma db seed && next build",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.14.3",
    "@trpc/client": "^10.0.0-proxy-beta.26",
    "@trpc/next": "^10.0.0-proxy-beta.26",
    "@trpc/react-query": "^10.0.0-proxy-beta.26",
    "@trpc/server": "^10.0.0-proxy-beta.26",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.2.2",
    "chart.js": "^4.2.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dayjs": "^1.11.7",
    "immutability-helper": "^3.1.1",
    "next": "12.3.1",
    "next-auth": "^4.15.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-select-async-paginate": "^0.7.2",
    "sass": "^1.55.0",
    "ts-node": "^10.9.1",
    "zod": "^3.20.5"
  },
  "devDependencies": {
    "@types/node": "18.8.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "typescript": "4.8.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.(ts|tsx)": "bash -c tsc --noEmit",
    "**/*.(ts|tsx|js)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": "prettier --write"
  }
}
